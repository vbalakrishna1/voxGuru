service cloud.firestore {
  match /databases/{database}/documents {
		match /app/{document=**} {
      allow read,write: if true;
    }
    match /users/{document=**} {
      allow read, write: if request.auth.uid != null;
      	// allow read , write: if true;
    }
    match /transaction/{document=**} {
      allow read, write: if request.auth.uid != null;
      	// allow read , write: if true;
    }
    match /liveClassRequest/{document=**} {
      allow write: if true;
    }
  }
}